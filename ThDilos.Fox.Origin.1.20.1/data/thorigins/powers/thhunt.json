{
    "type": "origins:multiple",
    "badges": [
        {
            "type": "origins:keybind",
            "sprite": "minecraft:textures/item/bone.png",
            "text": "origin.thorigin.sneak"
        }
    ],

    "double_tap_window":{
        "type": "origins:cooldown",
        "cooldown": 11,
        "hud_render": {
            "should_render": false
        }
    },

    "le_switch":{
        "type": "origins:toggle",
        "active_by_default": false,
        "key":"key.origins.denary_active",
        "retain_state": false
    },

    "activate_le_switch":{
        "type": "origins:active_self",
        "key": {
            "key": "key.sneak"
        },
        "entity_action": {
            "type": "origins:toggle",
            "power": "*:*_le_switch"
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:power_active",
                    "power": "le_switch",
                    "inverted": true
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_double_tap_window",
                    "comparison": ">",
                    "compare_to": 0
                }
            ]
        } 

    },

    "activate_cooldown": {
        "type": "origins:active_self",
        "key": {
            "key": "key.sneak"
        },
        "entity_action": {
            "type": "origins:delay",
            "ticks": 1,
            "action": {
                "type": "origins:trigger_cooldown",
                "power": "*:*_double_tap_window"
            }
        },
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_double_tap_window",
            "comparison": "==",
            "compare_to": 0
        }
    },

    "reset_le_switch": {
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:toggle",
            "power": "*:*_le_switch"
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:power_active",
                    "power": "*:*_le_switch"
                },
                {
                    "type": "origins:sneaking",
                    "inverted": true
                }
            ]
        }
    },

    "reset_if_not_during_window": {
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:trigger_cooldown",
            "power": "*:*_timer"
        },
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_le_switch",
            "inverted": true
        }
    },

    "timer":{
        "type": "origins:cooldown",
        "cooldown": 50,
        "hud_render": {
            "should_render": true,
            "bar_index": 3,
            "condition": {
                "type": "origins:and",
                "conditions": [
                    {
                        "type": "origins:sneaking"
                    },
                    {
                        "type": "origins:power_active",
                        "power": "*:*_le_switch"
                    }
                ]
            }
        }
    },

    "invisibility":{
        "type": "origins:invisibility",
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource":"*:*_timer",
                    "compare_to": 25,
                    "comparison": "<="
                }
            ]
        }
    },

    "start":{
        "type": "origins:active_self",
        "key": {
            "key": "key.sneak"
        },
        "entity_action": {
            "type": "origins:trigger_cooldown",
            "power": "*:*_timer"
        }
    },

    "resetwhenunsneak":{
        "type": "origins:action_over_time",
        "interval": 2,
        "entity_action": {
            "type": "origins:trigger_cooldown",
            "power": "*:*_timer"
        },
        "condition": { 
            "type": "origins:sneaking",
            "inverted": true
        }
    },

    "resetwhendash":{
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:trigger_cooldown",
            "power": "*:*_timer"
        },
        "cooldown": 30
    },

    "resetwhentakedamage":{
        "type": "origins:action_when_damage_taken",
        "damage_condition": {
            "type": "origins:or",
            "conditions":[
                {
                    "type": "origins:amount",
                    "comparison": ">=",
                    "compare_to": 3
                },
                {
                    "type": "origins:name",
                    "name": "onFire"
                },
                {
                    "type": "origins:name",
                    "name": "inFire"
                }
            ]
        },
        "entity_action": {
            "type": "origins:trigger_cooldown",
            "power": "*:*_timer"
        },
        "cooldown": 0
    },


    "resetwhenfall3blocks":{
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:trigger_cooldown",
            "power": "*:*_timer"
        },
        "condition": {
            "type": "origins:fall_distance",
            "comparison": ">=",
            "compare_to": 3
        }
    },

